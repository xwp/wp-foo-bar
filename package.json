{
  "name": "wp-foo-bar",
  "private": true,
  "title": "Foo Bar Plugin for WordPress",
  "description": "WordPress plugin template for extending Gutenberg at XWP.",
  "author": "XWP",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "amp",
    "amphtml",
    "wordpress",
    "wordpress-plugin"
  ],
  "homepage": "https://github.com/xwp/wp-foo-bar#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xwp/wp-foo-bar.git"
  },
  "bugs": {
    "url": "https://github.com/xwp/wp-foo-bar/issues"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6.9"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/polyfill": "7.8.3",
    "@wordpress/block-editor": "3.6.0",
    "@wordpress/browserslist-config": "2.6.0",
    "@wordpress/dom": "2.8.0",
    "@wordpress/dom-ready": "2.7.0",
    "@wordpress/eslint-plugin": "3.4.1",
    "@wordpress/i18n": "3.9.0",
    "@wordpress/scripts": "7.0.1",
    "@wordpress/server-side-render": "1.7.0",
    "@wordpress/url": "2.9.0",
    "accessible-autocomplete": "1.6.2",
    "autoprefixer": "9.7.4",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "browserslist": "4.8.6",
    "coveralls": "3.0.9",
    "cross-env": "7.0.0",
    "css-loader": "3.4.2",
    "cssnano": "4.1.10",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-react": "7.18.3",
    "eslint-utils": "2.0.0",
    "grunt": "1.0.4",
    "grunt-contrib-clean": "2.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-shell": "3.0.1",
    "grunt-wp-deploy": "2.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "npm-run-all": "4.1.5",
    "null-loader": "3.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss": "7.0.26",
    "postcss-color-function": "4.1.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.1",
    "postcss-preset-env": "6.7.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "rtlcss": "2.4.1",
    "rtlcss-webpack-plugin": "4.0.3",
    "semver": "7.1.2",
    "source-map-loader": "0.2.4",
    "svg-inline-loader": "0.8.0",
    "terser-webpack-plugin": "2.3.4",
    "uuid": "3.4.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpackbar": "4.0.0"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:prepare": "grunt clean",
    "build:css": "npm-run-all build:css:*",
    "build:css:copy": "cp -R assets/css/src/* assets/css",
    "build:css:rtlcss": "rtlcss -d assets/css/src assets/css -e '-rtl.css'",
    "build:js": "wp-scripts build",
    "build:run": "grunt build",
    "build:zip": "grunt create-build-zip",
    "dev": "wp-scripts start",
    "docker": "docker-compose run --rm --workdir=/var/www/html/wp-content/plugins/foo-bar wordpress",
    "env:logs": "bin/logs.sh",
    "env:start": "bin/start.sh",
    "env:stop": "bin/stop.sh",
    "format": "npm-run-all --parallel format:*",
    "format:css": "npm run lint:css -- --fix",
    "format:js": "wp-scripts format-js",
    "format:php": "composer format",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:php": "composer lint",
    "postinstall": "composer install",
    "readme": "composer readme",
    "test": "npm-run-all --parallel test:js test:php",
    "test-with-coverage": "npm-run-all --parallel test:js:coverage test:php:coverage",
    "test:js": "wp-scripts test-unit-js tests/js",
    "test:js:coverage": "wp-scripts test-unit-js --coverage --coverageDirectory=tests/coverage/js",
    "test:js:coveralls": "wp-scripts test-unit-js --coverage --coverageReporters=text-lcov | coveralls",
    "test:php": "composer test",
    "test:php:coverage": "composer test-coverage",
    "xdebug:status": "bin/xdebug.sh status",
    "xdebug:on": "bin/xdebug.sh on",
    "xdebug:off": "bin/xdebug.sh off"
  },
  "npmPackageJsonLintConfig": {
    "extends": "@wordpress/npm-package-json-lint-config",
    "rules": {
      "require-version": "off"
    }
  }
}
